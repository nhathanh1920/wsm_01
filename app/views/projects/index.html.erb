<div class="content">
  <div class="women_main">
    <div class="grids">
      <div class="panel panel-widget forms-panel">
        <div class="forms">
          <span>
            <%= link_to t(".new_project"), new_project_path,
              class:"btn btn-default" %>
          </span>
          <div class="form-grids widget-shadow" data-example-id="basic-forms">
            <div class="form-title">
              <h4><%= t ".show" %></h4>
            </div>
            <div class="form-body">
              <table id="table_list"
                class="table table-striped table-bordered">
                <thead class="thead-inverse">
                  <tr>
                    <th class="text-center"><%= t ".name" %></th>
                    <th class="text-center"><%= t ".abbrevation" %></th>
                    <th class="text-center"><%= t ".start_date" %></th>
                    <th class="text-center"><%= t ".end_date" %></th>
                    <th class="text-center" colspan="2"><%= t ".actions" %>
                    </th>
                  </tr>
                </thead>
                <tbody class="subject-index">
                  <%= render "project" %>
                </tbody>
              </table>
              <%= paginate @projects %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
